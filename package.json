{
  "version": "1.4.1",
  "name": "iterffect",
  "repository": {
    "url": "https://github.com/eloytoro/iterffect",
    "type": "git"
  },
  "description": "Define business logic in cancellable, forkable, asynchronous javascript generator definitions and promises",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnex:main": "es/index.js",
  "unpkg": "dist/iterffect.min.js",
  "author": "Eloy Toro <eloytoro@gmail.com> (https://github.com/eloytoro)",
  "engines": {
    "node": ">=6.1.0"
  },
  "files": [
    "dist",
    "src",
    "lib",
    "effects",
    "es"
  ],
  "scripts": {
    "clean": "rimraf ./lib ./es ./dist",
    "build:umd": "webpack --output-library-target=umd --output-filename=./dist/iterffect.js",
    "build:umd:min": "webpack --output-library-target=umd -p --output-filename=./dist/iterffect.min.js",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run clean && npm run build:cjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "test": "cross-env BABEL_ENV=test mocha --require=./test/setup.js --ui tdd",
    "prepare": "npm run build && npm run test"
  },
  "keywords": [
    "generator",
    "task",
    "sequence",
    "cancellable",
    "asynchronous",
    "promise",
    "process"
  ],
  "license": "MIT",
  "devDependencies": {
    "@reactivex/rxjs": "^5.4.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^5.0.5",
    "enzyme": "2",
    "enzyme-adapter-react-15": "^1.0.2",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.3",
    "mocha": "^4.0.1",
    "react": "15",
    "react-dom": "15",
    "regenerator-runtime": "^0.11.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.0.1",
    "sinon-chai": "^2.14.0",
    "webpack": "^3.7.1"
  }
}
