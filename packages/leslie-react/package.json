{
  "version": "0.0.0",
  "name": "leslie-react",
  "repository": {
    "url": "https://github.com/eloytoro/leslie",
    "type": "git"
  },
  "description": "Define business logic in cancellable, forkable, asynchronous hook tasks",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "jsnex:main": "esm/index.js",
  "author": "Eloy Toro <eloytoro@gmail.com> (https://github.com/eloytoro)",
  "files": [
    "README.md",
    "src",
    "lib",
    "esm"
  ],
  "scripts": {
    "clean": "rimraf ./lib ./esm",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir esm",
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "test": "jest",
    "prepare": "yarn build"
  },
  "keywords": [
    "generator",
    "task",
    "job",
    "react",
    "cancellable",
    "asynchronous",
    "promise",
    "process"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "cross-env": "^6.0.3",
    "jest": "^24.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.4",
    "leslie": "^0.0.0"
  },
  "jest": {
  }
}
